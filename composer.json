{
    "name": "zerodahero/laravel-workflow",
    "description": "Integerate Symfony Workflow component into Laravel.",
    "keywords": [
        "workflow",
        "symfony",
        "laravel",
        "laravel7",
        "laravel8"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2",
        "symfony/workflow": "^6.0|^7.0",
        "symfony/process": "^6.0|^7.0",
        "symfony/event-dispatcher-contracts": "^2.4|^3.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "ZeroDaHero\\LaravelWorkflow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "laravel": {
            "providers": [
                "ZeroDaHero\\LaravelWorkflow\\WorkflowServiceProvider"
            ],
            "aliases": {
                "Workflow": "ZeroDaHero\\LaravelWorkflow\\Facades\\WorkflowFacade"
            }
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^9.0|^10.5",
        "symfony/var-dumper": "^6.0|^7.0",
        "fakerphp/faker": "^1.13|^1.9.1",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "symfony/contracts": "^2.3"
    }
}
